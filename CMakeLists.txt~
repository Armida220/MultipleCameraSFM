cmake_minimum_required(VERSION 2.8)
project(SFM)
find_package(OpenCV REQUIRED)
include_directories(include)
set (
src
src/main.cpp
src/PnpSolver.cpp
src/Calibrator.cpp
src/FeaturesPts.cpp)
set(CMAKE_CXX_STANDARD 11)
add_executable(SFM ${src})
target_link_libraries(SFM ${OpenCV_LIBS})
message(目标是以科学的力量凌驾于超能力之上)
